 package collectionsession;

import java.util.ArrayList;

public class ArrayListconcept {

	public static void main(String[] args) {
	int a[]=new int[3];
	ArrayList ar=new ArrayList();
	ar.add(10);//0
	ar.add(20);//1
	ar.add(30);//2
	System.out.println(ar.size());
	ar.add(40);//3
	ar.add(50);//4
	ar.add(50);//5
	System.out.println(ar.size());
	System.out.println(ar.get(4));
	

	}

}
